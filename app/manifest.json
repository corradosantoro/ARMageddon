{
  "name": "ARMageddon",
  "description": "ARM-based Computer Emulator",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background" : {
      "scripts": ["armageddon.js"]
    }
  },
  "permissions": [
        "serial",
        "usb",
        "storage",
        "fileSystem",
        "fileSystem.write",
        "fileSystem.retainEntries",
        "notifications",
        "alwaysOnTopWindows"  ]
}
